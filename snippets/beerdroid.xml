<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- Github (Giste) -->
  <remote name="giste"
          fetch="https://github.com/giste/"
          revision="refs/heads/14.0" />

  <!-- Remove from crDroid -->
  <remove-project name="crdroidandroid/android_lineage-sdk" />
  <remove-project name="crdroidandroid/android_packages_apps_LineageParts" />
  <remove-project name="crdroidandroid/android_vendor_crdroid" />
  <remove-project name="crdroidandroid/android_vendor_addons" />

  <!-- Beerdroid additions -->
  <project path="lineage-sdk" name="beerdroid_lineage-sdk" remote="giste" />
  <project path="packages/apps/LineageParts" name="beerdroid_packages_apps_LineageParts" remote="giste" />
  <project path="vendor/lineage" name="beerdroid_vendor_beerdroid" remote="giste" />
  <project path="vendor/addons" name="beerdroid_vendor_addons" remote="giste" />

</manifest>
